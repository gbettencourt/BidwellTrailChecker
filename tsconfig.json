{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"outDir": "dist",
		"baseUrl": "."
	},
	"include": ["*.ts"],
	"exclude": ["client"]
}
